{
  "name": "mamokin",
  "version": "0.5.1",
  "description": "Mamokin Identity [ Gatsby.js V2 starter template based on Identity by HTML5 UP ]",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mamokin/mamokin.git"
  },
  "author": {
    "name": "Anubhav Srivastava",
    "email": "anubhav.srivastava00@gmail.com"
  },
  "contributors": [
    "Michael Seaward <m.seaward@mamok.in> (mamok.in)"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "base32.js": "^0.1.0",
    "chance": "^1.1.4",
    "chroma-js": "^2.1.0",
    "crc": "^3.8.0",
    "gatsby": "^2.4.3",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "highlight.js": "^9.18.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-select": "^3.0.8",
    "sha.js": "^2.4.11",
    "smooth-scrollbar": "^8.5.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "deploy": "npm run clean && gatsby build --prefix-paths && gh-pages -d public",
    "serve": "gatsby serve",
    "clean": "rimraf .cache public",
    "format": "prettier --write '**/*.js'",
    "eslint": "node_modules/.bin/eslint src/**/*.{js,jsx} --fix",
    "stylelint": "stylelint **/*.scss --fix",
    "get-next-version": "node ./get-next-version.js",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserifyNodeCrypto": "browserify ./src/services/getNodeCrypto.js > ./src/services/NodeCryptoBundled.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.17.0",
    "react-hook-form": "^4.10.1",
    "rimraf": "^2.6.3",
    "standard-version": "^7.1.0",
    "stylelint": "^13.2.1",
    "stylelint-order": "^4.0.0",
    "undefined": "^0.1.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "gatsby-starter-identity"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mamokin/mamokin/issues"
  },
  "homepage": "https://github.com/mamokin/mamokin#readme",
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}
